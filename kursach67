#include <iostream>
#include <math.h>
using namespace std;

int main(){
    int n;
    cout << "n = ";
    cin >> n;
    int array[n];
    for (int i = 0; i < n; i++)
    {
        cout << "a" << i << " = ";
        cin >> array[i];
    }
    for(int i = 1; i < n; ++i)
    {
        for(int r = 0; r < n-i; r++)
        {
            if(array[r] < array[r+1])
            {
                int temp = array[r];
                array[r] = array[r+1];
                array[r+1] = temp;
            }
        }
    }
    for (int i = 0; i < n; i++)
    {
        cout << "a" << i << " = " << array[i] << " ";
    }
}
