#include <iostream>
#include <math.h>
using namespace std;

int main(){
    int n,m;
    double max;
    int indexMax1;
    int indexMax2;
    cout << "n = ";
    cin >> n; 
    cout << "m = ";
    cin >> m;

    double arrayX[n], arrayY[m];
    double arrayX2[n-1], arrayY2[m-1];

    for (int i = 0; i < n; i++)
    {
        cout << "x" << i << " = ";
        cin >> arrayX[i];
        if (arrayX[i] > max)
        {
            max = arrayX[i];
            indexMax1 = i;
        }
    }
    for (int i = 0; i < n-1; i++)
    {
        if (i == indexMax1)
        {
            arrayX2[i] = arrayX[i+1];
        }
        arrayX2[i] = arrayX[i];
    }
    for (int i = 0; i < m; i++)
    {
        cout << "y" << i << " = ";
        cin >> arrayY[i];
        if (arrayY[i] > max)
        {
            max = arrayY[i];
            indexMax2 = i;
        }
    }
    max = 0;
    for (int i = 0; i < m-1; i++)
    {
        if (i == indexMax2)
        {
            arrayY2[i] = arrayY[i+1];
        }
        arrayY2[i] = arrayY[i];
    }
    for (int i = 0; i < n-1; i++)
    {
        cout << "a" << i << " = " << arrayX2[i] << " ";
    }
    for (int i = 0; i < m-1; i++)
    {
        cout << "a" << i << " = " << arrayY2[i] << " ";
    }
}
