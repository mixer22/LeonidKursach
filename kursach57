#include <iostream>
#include <math.h>
using namespace std;

int main(){
    int n,m;
    double max;
    int indexMax1;
    int indexMax2;
    cout << "n = ";
    cin >> n;
    cout << "m = ";
    cin >> m;
    const int n1 = n;
    const int m1 = m;
    const int n2 = n1 -1;
    const int m2 = m1 - 1;
    double arrayX[n1], arrayY[m1];
    double arrayX2[n2], arrayY2[m2];

    for (int i = 0; i < n1; i++)
    {
        cout << "x" << i << " = ";
        cin >> arrayX[i];
        if (arrayX[i] > max)
        {
            max = arrayX[i];
            indexMax1 = i;
        }
    }
    for (int i = 0; i < n2; i++)
    {
        if (i == indexMax1)
        {
            arrayX2[i] = arrayX[i+1];
        }
        arrayX2[i] = arrayX[i];
    }
    for (int i = 0; i < m1; i++)
    {
        cout << "y" << i << " = ";
        cin >> arrayY[i];
        if (arrayY[i] > max)
        {
            max = arrayY[i];
            indexMax2 = i;
        }
    }
    max = 0;
    for (int i = 0; i < m2; i++)
    {
        if (i == indexMax2)
        {
            arrayY2[i] = arrayY[i+1];
        }
        arrayY2[i] = arrayY[i];
    }
    for (int i = 0; i < n2; i++)
    {
        cout << "a" << i << " = " << arrayX2[i] << " ";
    }
    for (int i = 0; i < m2; i++)
    {
        cout << "a" << i << " = " << arrayY2[i] << " ";
    }
}
