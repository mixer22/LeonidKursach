#include <iostream>
#include <math.h>
using namespace std;

int main(){
    int n, count = 0;
    cout << "n = ";
    cin >> n;
    double array[n], array1[n];
    for (int i = 0; i < n; i++)
    {
        cout << "a" << i << " = ";
        cin >> array[i];

    }
    for (int i = n-1 ; i >= 0; i--)
    {
        if (array[i] < 0)
        {
            array1[count] = array[i];
            count++;
        }
    }
    for (int i = 0; i < n; i++)
    {
        if (array[i] >= 0)
        {
            array1[count] = array[i];
            count++;
        }
    }
    for (int i = 0; i < n; i++)
    {
        cout << "a" << i << " = " << array1[i] << " ";
    }
}
