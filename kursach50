#include <iostream>
using namespace std;
int main()
{
	unsigned long n = 0, mul = 1;
	cout << "input n: ";
	cin >> n;
	int *arr = new int[n];
	cout << endl << "input array elements: " << endl;
	for (int i = 0; i < n; i++)
		cin >> arr[i];
	
	for (int i = 1,  j = n - 1; i < n; i++, j--)
	{
		mul *= arr[i] + arr[i - 1] + 2 *arr[j];
	}
	cout << endl << "answer: " << mul;
}
