#include <iostream>
#include <math.h>
using namespace std;

int main(){
    int n, count = 0, k = 0;
    cout << "n = ";
    cin >> n;
    double *arrayA = new double[n];
    for (int i = 0; i < n; i++)
    {
        cout << "a" << i << " = ";
        cin >> arrayA[i];
        if (arrayA[i] > 0)
        {
            k++;
        }
    }
    double arrayX[k];
    for (int i = n-1; i >= 0; i--)
    {
        if (arrayA[i] > 0)
        {
            arrayX[count] = arrayA[i];
            count++;
        }
    }
    for (int i = 0; i < k; i++)
    {
        cout << "x" << i << " = " << arrayX[i] << " ";
    }
}
