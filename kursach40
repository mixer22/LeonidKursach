 #include <iostream>
using namespace std;
int main()
{
	int n = 0, even = 0, noteven = 0, cX = 0, cY = 0;
	cout << "input n: ";
	cin >> n;
	int* outArr = new int[n], *arr = new int[n], *arrX = new int[n], *arrY = new int[n];
	cout << "input array elements: " << endl;

	for (int i = 0; i < n; i++)
	{
		cin >> arr[i];
		if (arr[i] % 2 == 0)
		{
			arrX[cX++] = arr[i];
		}
		else
			arrY[cY++] = arr[i];
	}
	int i = 0, counter = 0;
	if (cX > cY)
		i = cY;
	else
		if (cY > cX)
			i = cX;
		else if (cY == cX)
			i = cX;
	for (int j = 0; j < i; j++)
	{
		cout << arrX[j] << ' ' << arrY[j] << ' ';
	}
}
